#!/bin/bash
machinedescription="For a a $(sysctl -n hw.model) with $(sysctl -n hw.machine) architecture running $(sw_vers -productName) $(sw_vers -productVersion) ,"

# execute file suggestCommand.js that is in the directory the script is run from with node , even if it is a symlink
node "$(dirname "$(readlink -f "$0")")/../js/suggestCommand.js" "$machinedescription write a fish command line (executed in fish) in one line to do the following: $*"
